{
  "expo": {
    "name": "kedu",
    "slug": "kedu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whiteicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/whitelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#008080"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/whitelogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.geepytech.kedu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/whitelogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "443885d6-d5e4-4373-9c14-53c162c59181"
      }
    }
  }
}
